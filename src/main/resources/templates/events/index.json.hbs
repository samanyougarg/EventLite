{
  "title": "EventLite Events",
  "_self": "{{self_link}}events",
  "events": [
  	{{#events}}
  	{
  		"id": {{id}},
  		"name": "{{name}}",
  		"date": "{{date}}",
  		"time": "{{time}}",
  		"venue": {
  		{{#venue}}
  			"id": {{id}},
  			"name": "{{name}}",
  			"capacity": {{capacity}},
  			"_self": "{{self_link}}venues/{{id}}"
  		{{/venue}}
   		},
   		"_self": "{{self_link}}events/{{id}}"
   	},
  	{{/events}}
  	]

}
  